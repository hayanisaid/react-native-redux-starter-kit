{
  "name": "ReactNativeStarterKit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "REACT_NATIVE_FSOP_TIMEOUT=60000 NODE_ENV=development node node_modules/react-native/local-cli/cli.js start",
    "clean": "npm start -- --reset-cache",
    "react-devtool": "react-devtools",
    "redux-devtool": "remotedev",
    "test:watch": "npm run test -- --watch --maxWorkers=1 --bail",
    "flow": "flow",
    "flow:check": "flow check",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react": "^16.0.0-beta.5",
    "react-native": "^0.54.0",
    "react-native-firebase": "^3.0.3",
    "react-native-router-flux": "^4.0.0-beta.28",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.10.1",
    "redux-persist-migrate": "^5.0.0",
    "redux-persist-transform-filter": "0.0.15",
    "redux-saga": "^0.15.6",
    "redux-thunk": "^2.2.0",
    "remote-redux-devtools": "^0.5.12"
  },
  "devDependencies": {
    "babel-eslint": "8.0.1",
    "babel-jest": "21.2.0",
    "babel-plugin-transform-decorators": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-jest": "21.2.0",
    "babel-preset-react-native": "4.0.0",
    "babel-preset-react-native-stage-0": "1.0.1",
    "enzyme": "3.1.0",
    "eslint": "4.8.0",
    "eslint-config-airbnb": "16.0.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "eslint-plugin-react-native": "3.1.0",
    "flow-bin": "0.56.0",
    "flow-typed": "2.2.0",
    "husky": "0.14.3",
    "jest": "21.2.1",
    "jest-enzyme": "4.0.1",
    "jest-repl": "21.2.1",
    "react-addons-test-utils": "15.6.2",
    "react-apollo": "1.4.16",
    "react-devtools": "2.5.2",
    "react-native-mock": "0.3.1",
    "react-test-renderer": "16.0.0-beta.5",
    "remotedev-server": "0.2.4",
    "sc-broker-cluster": "4.3.0"
  },
  "jest": {
    "preset": "react-native",
    "bail": true,
    "verbose": true,
    "automock": false,
    "moduleNameMapper": {
      "^React$": "<rootDir>/node_modules/react"
    },
    "haste": {
      "defaultPlatform": "ios",
      "platforms": [
        "android",
        "ios",
        "native"
      ],
      "providesModuleNodeModules": [
        "react-native"
      ]
    }
  }
}
